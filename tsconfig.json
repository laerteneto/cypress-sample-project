{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2015",
    "outDir": "./dist",
    "types": ["cypress"],
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "strictNullChecks": true,
    "sourceMap": false
  },
  "include": ["./cypress", "**/*.ts"],
  "exclude": ["node_modules"]
}
